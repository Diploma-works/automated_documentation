{
  "name": "automated-documentation-for-java",
  "displayName": "Automated documentation for Java",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "automated-documentation-for-java.generateDocumentationFile",
        "title": "Generate Documentation File"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename =~ /.+\\.java/",
          "command": "automated-documentation-for-java.generateDocumentationFile",
          "group": "5_cutcopypaste"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/traverse": "^7.24.0",
    "@supabase/supabase-js": "^2.39.8",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "java-ast": "^0.3.0",
    "java-parser": "~2.2.0",
    "jsonpath": "^1.1.1",
    "langchain": "^0.1.28"
  }
}
